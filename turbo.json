{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/staging",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", ".expo/**", ".android/**", ".ios/**"]
    },
    "start": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
