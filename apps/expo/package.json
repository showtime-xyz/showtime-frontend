{
  "main": "index.js",
  "name": "@showtime/expo",
  "scripts": {
    "waldo:build": "E2E=true FLIPPER_DISABLE=1 STAGE=development expo prebuild --skip-dependency-update react",
    "dev": "yarn start:dev-client",
    "start": "STAGE=development expo start --dev-client",
    "start:dev-client": "STAGE=development yarn expo:prebuild && yarn expo start --dev-client",
    "start:dev-client:clear": "yarn start:dev-client -c",
    "eject": "expo eject",
    "expo:prebuild": "yarn expo prebuild --skip-dependency-update react",
    "ios": "STAGE=development yarn expo:prebuild && STAGE=development EXPO_USE_APPLE_DEVICE=0 yarn expo run:ios",
    "ios:beta": "STAGE=development EXPO_BETA=1 yarn expo:prebuild && STAGE=development EXPO_BETA=1 EXPO_USE_APPLE_DEVICE=0 yarn expo run:ios",
    "e2e:ios": "DETOX=true yarn expo:prebuild && yarn e2e:ios:build && yarn e2e:ios:test",
    "e2e:ios:build": "yarn detox build --configuration ios.sim.debug",
    "e2e:ios:test": "yarn detox test --configuration ios.sim.debug",
    "e2e:ios:release": "yarn e2e:ios:build:release && yarn e2e:ios:test:release",
    "e2e:ios:build:release": "yarn detox build --configuration ios.sim.release",
    "e2e:ios:test:release": "yarn detox test --configuration ios.sim.release",
    "android": "STAGE=development yarn expo:prebuild && STAGE=development yarn expo run:android",
    "android:beta": "STAGE=development EXPO_BETA=1 yarn expo:prebuild && STAGE=development EXPO_BETA=1 yarn expo run:android",
    "e2e:android": "DETOX=true yarn expo:prebuild && yarn e2e:android:build && yarn e2e:android:test",
    "e2e:android:build": "yarn detox build --configuration android.emu.debug",
    "e2e:android:test": "yarn detox test --configuration android.emu.debug",
    "e2e:android:release": "yarn e2e:android:build:release && yarn e2e:android:test:release",
    "e2e:android:build:release": "yarn detox build --configuration android.emu.release",
    "e2e:android:test:release": "yarn detox test --configuration android.emu.release",
    "postinstall": "cd ../.. && yarn patch-package",
    "update:development": "STAGE=development eas update --branch default",
    "update:staging": "STAGE=staging eas update --branch staging",
    "update:production": "STAGE=production eas update --branch production",
    "build:development": "STAGE=development eas build --profile development --platform all && say done",
    "build:development:ios": "STAGE=development eas build --profile development --platform ios && say done",
    "build:development:android": "STAGE=development eas build --profile development --platform android && say done",
    "build:preview": "STAGE=staging eas build --profile preview --platform all && say done",
    "build:preview:ios": "STAGE=staging eas build --profile preview --platform ios && say done",
    "build:preview:android": "STAGE=staging eas build --profile preview --platform android && say done",
    "build:production": "STAGE=production eas build --profile production --platform all && say done",
    "build:production:ios": "STAGE=production eas build --profile production --platform ios && say done",
    "build:production:android": "STAGE=production eas build --profile production --platform android && say done",
    "build:simulator": "STAGE=staging eas build --profile simulator --platform ios && say done",
    "submit:ios": "STAGE=production eas submit --platform ios",
    "submit:android": "STAGE=production eas submit --platform android",
    "deploy:production": "echo 'Do not do this unless you know what you are doing!' && yarn update:production --message 'Deploy to production' && yarn build:production --auto-submit && say done",
    "adb:reverse": "adb reverse tcp:3000 tcp:3000",
    "test": "jest ./component-tests",
    "bump-application-version": "zx scripts/bump-version.mjs"
  },
