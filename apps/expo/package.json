{
  "main": "index.js",
  "name": "@showtime/expo",
  "scripts": {
    "E2E:build": "E2E=true FLIPPER_DISABLE=1 STAGE=development expo prebuild --skip-dependency-update react-native,react",
    "E2E:build:ios": "E2E=true FLIPPER_DISABLE=1 STAGE=development expo prebuild --platform ios --skip-dependency-update react-native,react",
    "E2E:build:android": "E2E=true FLIPPER_DISABLE=1 STAGE=development expo prebuild --platform android --skip-dependency-update react-native,react",
    "dev": "yarn start:dev-client",
    "start": "EXPO_NO_DOTENV=1 STAGE=development expo start --dev-client",
    "start:prod": "EXPO_NO_DOTENV=1 npx expo start --no-dev --minify",
    "build:release:local:android": "yarn run android --variant Release",
    "build:release:local:ios": "yarn run ios --configuration Release",
    "start:dev-client": "EXPO_NO_DOTENV=1 STAGE=development yarn expo:prebuild && yarn expo start --dev-client",
    "start:dev-client:clear": "yarn start:dev-client -c",
    "eject": "expo eject",
    "expo:prebuild": "yarn expo prebuild --skip-dependency-update react-native,react",
    "ios": "EXPO_NO_DOTENV=1 STAGE=development yarn expo:prebuild && STAGE=development EXPO_USE_APPLE_DEVICE=0 yarn expo run:ios",
    "ios:beta": "EXPO_NO_DOTENV=1 STAGE=development EXPO_BETA=1 yarn expo:prebuild && STAGE=development EXPO_BETA=1 EXPO_USE_APPLE_DEVICE=0 yarn expo run:ios",
    "android": "EXPO_NO_DOTENV=1 STAGE=development yarn expo:prebuild && STAGE=development yarn expo run:android",
    "android:beta": "EXPO_NO_DOTENV=1 STAGE=development EXPO_BETA=1 yarn expo:prebuild && STAGE=development EXPO_BETA=1 yarn expo run:android",
    "postinstall": "cd ../.. && yarn patch-package",
    "update:development": "EXPO_NO_DOTENV=1 STAGE=development eas update --branch default",
    "update:staging": "EXPO_NO_DOTENV=1 STAGE=staging eas update --branch staging",
    "update:production": "EXPO_NO_DOTENV=1 STAGE=production eas update --branch production",
    "build:development": "EXPO_NO_DOTENV=1 STAGE=development eas build --profile development --platform all && say done",
    "build:development:ios": "EXPO_NO_DOTENV=1 STAGE=development eas build --profile development --platform ios && say done",
    "build:development:android": "EXPO_NO_DOTENV=1 STAGE=development eas build --profile development --platform android && say done",
    "build:preview": "EXPO_NO_DOTENV=1 STAGE=staging eas build --profile preview --platform all && say done",
    "build:preview:ios": "EXPO_NO_DOTENV=1 STAGE=staging eas build --profile preview --platform ios && say done",
    "build:preview:android": "EXPO_NO_DOTENV=1 STAGE=staging eas build --profile preview --platform android && say done",
    "build:production": "EXPO_NO_DOTENV=1 STAGE=production eas build --profile production --platform all && say done",
    "build:production:ios": "EXPO_NO_DOTENV=1 STAGE=production eas build --profile production --platform ios && say done",
    "build:production:android": "EXPO_NO_DOTENV=1 STAGE=production eas build --profile production --platform android && say done",
    "build:simulator": "EXPO_NO_DOTENV=1 STAGE=staging eas build --profile simulator --platform ios && say done",
    "submit:ios": "EXPO_NO_DOTENV=1 STAGE=production eas submit --platform ios",
    "submit:android": "EXPO_NO_DOTENV=1 STAGE=production eas submit --platform android",
    "deploy:production": "echo 'Do not do this unless you know what you are doing!' && yarn update:production --message 'Deploy to production' && yarn build:production --auto-submit && say done",
    "adb:reverse": "adb reverse tcp:3000 tcp:3000",
    "test": "jest ./component-tests",
    "bump-application-version": "zx scripts/bump-version.mjs"
  },
