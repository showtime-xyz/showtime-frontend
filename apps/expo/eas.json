{
  "build": {
    "production": {
      "releaseChannel": "production",
      "distribution": "store",
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "STAGE": "production"
      },
      "node": "14.18.1",
      "cache": {
        "key": "showtime-2"
      }
    },
    "preview": {
      "releaseChannel": "staging",
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "STAGE": "staging"
      },
      "node": "14.18.1",
      "cache": {
        "key": "showtime-2"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      },
      "env": {
        "STAGE": "development"
      },
      "node": "14.18.1",
      "cache": {
        "key": "showtime-2"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./service-account-key.json",
        "track": "beta"
      },
      "ios": {
        "appName": "Showtime: NFT Social Network",
        "ascAppId": "1606611688",
        "appleId": "axeldelafosse@gmail.com"
      }
    }
  },
  "cli": {
    "version": ">= 0.34.0"
  }
}
