{
  "build": {
    "production": {
      "channel": "production",
      "distribution": "store",
      "ios": {
        "image": "latest",
        "resourceClass": "large"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest",
        "resourceClass": "large"
      },
      "env": {
        "STAGE": "production",
        "FLIPPER_DISABLE": "1",
        "EX_UPDATES_ANDROID_DELAY_LOAD_APP": "false"
      },
      "prebuildCommand": "prebuild --skip-dependency-update react",
    },
    "preview": {
      "channel": "staging",
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "resourceClass": "large"
      },
      "android": {
        "buildType": "apk",
        "image": "latest",
        "resourceClass": "large"
      },
      "env": {
        "STAGE": "staging",
        "FLIPPER_DISABLE": "1",
        "EX_UPDATES_ANDROID_DELAY_LOAD_APP": "false"
      },
      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "showtime-3"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "resourceClass": "large"
      },
      "android": {
        "image": "latest",
        "resourceClass": "large"
      },
      "env": {
        "STAGE": "development",
        "EX_UPDATES_ANDROID_DELAY_LOAD_APP": "false"
      },
      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "showtime-3"
      }
    },
    "simulator": {
      "ios": {
        "simulator": true,
        "image": "latest",
        "resourceClass": "large"
      },
      "android": {
        "image": "latest",
        "resourceClass": "large"
      },
      "env": {
        "STAGE": "staging",
        "EX_UPDATES_ANDROID_DELAY_LOAD_APP": "false"
      },
      "prebuildCommand": "prebuild --skip-dependency-update react",
      "cache": {
        "key": "showtime-3"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./service-account-key.json"
      },
      "ios": {
        "appName": "Showtime: NFT Social Network",
        "ascAppId": "1606611688",
        "appleId": "axeldelafosse@gmail.com"
      }
    }
  },
  "cli": {
    "version": ">= 0.52.0"
  }
}