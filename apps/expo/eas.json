{
	"build": {
		"production": {
			"releaseChannel": "production",
			"distribution": "store",
			"ios": {
				"autoIncrement": true
			},
			"android": {
				"autoIncrement": true,
				"buildType": "app-bundle"
			},
			"env": {
				"STAGE": "production"
			},
			"node": "14.18.1"
		},
		"preview": {
			"releaseChannel": "staging",
			"distribution": "internal",
			"android": {
				"buildType": "apk"
			},
			"env": {
				"STAGE": "staging"
			},
			"node": "14.18.1"
		},
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"STAGE": "development"
			},
			"node": "14.18.1"
		}
	},
	"cli": {
		"version": ">= 0.34.0"
	}
}
