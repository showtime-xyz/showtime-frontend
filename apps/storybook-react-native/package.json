    "react-native-web": "0.18.7",
    "three": "^0.142.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@types/three": "^0.142.0",
    "typescript": "4.7.4"
  },
  "private": true
}
