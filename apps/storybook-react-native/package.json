{
  "name": "@showtime/storybook-react-native",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "yarn start:dev-client",
    "start": "expo start --dev-client",
    "start:dev-client": "yarn expo:prebuild && expo start --dev-client --port=8083",
    "start:dev-client:clear": "yarn start:dev-client -c",
    "eject": "expo eject",
    "expo:prebuild": "expo prebuild --skip-dependency-update react",
    "ios": "yarn expo:prebuild && EXPO_USE_APPLE_DEVICE=0 expo run:ios",
    "android": "yarn expo:prebuild && expo run:android",
    "postinstall": "cd ../.. && yarn patch-package",
    "update:development": "STAGE=development eas update --branch default",
    "build": "yarn build:development",
    "build:development": "STAGE=development eas build --profile development --platform all",
    "build:development:ios": "STAGE=development eas build --profile development --platform ios",
    "build:development:android": "STAGE=development eas build --profile development --platform android",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher"
  },
  "dependencies": {
    "@coinbase/wallet-mobile-sdk": "^1.0.4",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@react-native-community/datetimepicker": "6.7.5",
    "@react-native-community/slider": "4.4.2",
    "@react-native-menu/menu": "^0.7.3",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native-stack": "^6.9.1",
    "@react-three/fiber": "^8.2.1",
    "@shopify/flash-list": "^1.4.1",
    "@storybook/addon-actions": "~6.5.16",
    "@storybook/addon-controls": "~6.3.12",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "design-system": "*",
    "expo": "^48.0.4",
    "expo-asset": "~8.6.2",
    "expo-av": "~13.2.1",
    "expo-blur": "~12.2.2",
    "expo-constants": "~14.2.1",
    "expo-dev-client": "~2.1.3",
    "expo-font": "~11.1.1",
    "expo-gl": "~12.4.0",
    "expo-haptics": "~12.2.1",
    "expo-image-picker": "~14.1.1",
    "expo-linear-gradient": "~12.1.1",
    "expo-linking": "~4.0.1",
    "expo-modules-core": "~1.2.3",
    "expo-splash-screen": "~0.18.1",
    "expo-status-bar": "~1.4.4",
    "expo-system-ui": "~2.2.1",
    "expo-updates": "~0.16.3",
    "expo-web-browser": "~12.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.71.3",
    "react-native-blob-jsi-helper": "^0.2.3",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "^1.8.0",
    "react-native-ios-context-menu": "^1.15.3",
    "react-native-mmkv": "2.6.2",
    "react-native-pager-view": "6.1.2",
    "react-native-quick-base64": "^2.0.5",
    "react-native-reanimated": "~2.14.4",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "13.8.0",
    "react-native-web": "~0.18.10",
    "three": "^0.142.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/three": "^0.142.0",
    "typescript": "^4.9.5"
  },
  "private": true
}
