{
  "name": "@showtime/storybook-react-native",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "yarn start:dev-client",
    "start": "expo start --dev-client",
    "start:dev-client": "yarn expo:prebuild && expo start --dev-client --port=8083",
    "start:dev-client:clear": "yarn start:dev-client -c",
    "eject": "expo eject",
    "expo:prebuild": "expo prebuild --skip-dependency-update react && yarn react-native link",
    "ios": "yarn expo:prebuild && EXPO_USE_APPLE_DEVICE=0 expo run:ios",
    "android": "yarn expo:prebuild && expo run:android",
    "postinstall": "cd ../.. && yarn patch-package",
    "update:development": "STAGE=development eas update --branch default",
    "build": "yarn build:development",
    "build:development": "STAGE=development eas build --profile development --platform all",
    "build:development:ios": "STAGE=development eas build --profile development --platform ios",
    "build:development:android": "STAGE=development eas build --profile development --platform android",
    "prestart": "sb-rn-get-stories",
    "storybook-watcher": "sb-rn-watcher",
    "eas-build-post-install": "react-native link"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "@react-native-async-storage/async-storage": "~1.17.0",
    "@react-native-community/datetimepicker": "^6.1.0",
    "@react-native-community/slider": "^4.2.1",
    "@react-native-menu/menu": "^0.5.2",
    "@react-native-segmented-control/segmented-control": "^2.4.0",
    "@react-navigation/native": "^6.0.7",
    "@react-navigation/native-stack": "^6.3.0",
    "@react-three/fiber": "^8.0.12",
    "@storybook/addon-actions": "~6.3.12",
    "@storybook/addon-controls": "~6.3.12",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "@zeego/dropdown-menu": "^0.0.2-alpha.28",
    "design-system": "*",
    "expo": "next",
    "expo-asset": "~8.4.4",
    "expo-av": "~11.2.2",
    "expo-blur": "~11.1.0",
    "expo-community-flipper": "^44.0.1",
    "expo-constants": "~13.0.1",
    "expo-dev-client": "~0.8.4",
    "expo-font": "~10.0.4",
    "expo-gl": "~11.0.3",
    "expo-image-picker": "^12.0.2",
    "expo-linear-gradient": "~11.3.0",
    "expo-linking": "~3.0.0",
    "expo-modules-core": "~0.6.4",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-system-ui": "~1.1.0",
    "expo-updates": "~0.11.6",
    "expo-web-browser": "~10.1.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.64.3",
    "react-native-blob-jsi-helper": "^0.2.1",
    "react-native-cronet": "^0.5.0",
    "react-native-fast-image": "^8.5.11",
    "react-native-flipper": "^0.131.1",
    "react-native-gesture-handler": "2.4.1",
    "react-native-ios-context-menu": "1.3.0",
    "react-native-mmkv": "^1.6.2",
    "react-native-pager-view": "^5.4.9",
    "react-native-quick-base64": "^2.0.2",
    "react-native-random-values-jsi-helper": "^1.0.3",
    "react-native-randombytes": "^3.6.1",
    "react-native-reanimated": "2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-vision-camera": "2.12.0",
    "react-native-web": "0.17.7",
    "three": "^0.139.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/three": "^0.139.0",
    "typescript": "4.5.2"
  },
  "private": true
}
